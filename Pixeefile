import pandas as pd

# File paths (replace 'input.xlsx' with your actual input file path)
input_file = 'input.xlsx'
output_file = 'output_filtered.xlsx'

# Read the Excel file
df = pd.read_excel(input_file)

# Filter out rows where 'creator' is 'Ram Singh' or blank (NaN or empty string)
filtered_df = df[~((df['creator'] == 'Ram Singh') | (df['creator'].isna()) | (df['creator'] == ''))]

# Save the filtered DataFrame to a new Excel file
filtered_df.to_excel(output_file, index=False)

print(f"Filtered data saved to {output_file}")