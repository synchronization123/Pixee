import pandas as pd

# Load the Excel file (replace 'input.xlsx' with your actual file path)
df = pd.read_excel('input.xlsx')

# Assuming the columns are named exactly as specified, with spaces
# e.g., 'Trace', 'application id', 'orgUuid', 'created date 1', etc.

# Loop through each set from 1 to 10
for i in range(1, 11):
    # Select the relevant columns for this set
    cols = ['Trace', 'application id', 'orgUuid', f'created date {i}', f'creator {i}', f'note {i}']
    
    # Check if all columns exist, skip if not
    if all(col in df.columns for col in cols):
        temp_df = df[cols].copy()
        
        # Rename the set-specific columns to generic names for output
        temp_df.columns = ['Trace', 'application id', 'orgUuid', 'created date', 'creator', 'note']
        
        # Iterate over each row and print the output
        for _, row in temp_df.iterrows():
            # Handle NaN values by replacing with empty string or appropriate placeholder
            values = [str(row.get(col, '')) if pd.notna(row.get(col)) else '' for col in temp_df.columns]
            print(', '.join(values))
    else:
        print(f"Set {i} columns not found, skipping.")